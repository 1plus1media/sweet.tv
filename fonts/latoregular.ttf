                            <script>
                                document.querySelector('.tabs-close').addEventListener('click', function () {
                                    document.querySelectorAll('.accept_geo').forEach(function (item) {
                                        item.style.display = 'none';
                                    });
                                });

                                document.addEventListener("DOMContentLoaded", function () {
                                    if(document.querySelector('body').offsetWidth > 1024){
                                        $(window).click(function() {
                                            if($('.accept_geo').css('display','block')){
                                                $('.accept_geo').css('display','none');
                                            }
                                        });

                                        $('.accept_geo').click(function(event){
                                            event.stopPropagation();
                                        });

                                        $('.geo_pos').hover(function(event) {
                                            $('.accept_geo').css('display','block');

                                            setTimeout(function () {
                                                $('.accept_geo').css('display','none');
                                            }, 2000);

                                            if (event.target.id === "btn-close") {
                                                document.querySelectorAll('.accept_geo').forEach(function (item) {
                                                    item.style.display = 'none';
                                                });
                                            }
                                        });
                                    }

                                    if(document.querySelector('body').offsetWidth <= 1024){
                                        if($('.accept_geo').css('display','block')){
                                            setTimeout(function () {
                                                $('.accept_geo').css('display','none');
                                                $('.accept_geo').removeClass('d-block');
                                            }, 2000);
                                        }

                                        $(window).click(function(event) {
                                            if($('.accept_geo').css('display','block')){
                                                $('.accept_geo').css('display','none');
                                                $('.accept_geo').removeClass('d-block');
                                                if (event.target.classList.contains('geo_pos')) {
                                                    $('.accept_geo').toggleClass('d-block');
                                                    setTimeout(function () {
                                                        $('.accept_geo').css('display','none');
                                                        $('.accept_geo').removeClass('d-block');
                                                    }, 2000);
                                                }
                                                if (event.target.id === "btn-close") {
                                                    $('.accept_geo').css('display','none');
                                                    $('.accept_geo').removeClass('d-block');
                                                }
                                            }
                                        });

                                        $('.accept_geo, .geo_pos').click(function(event){
                                            if (!event.target.classList.contains('geo_pos')) {
                                                setTimeout(function () {
                                                    $('.accept_geo').css('display','none');
                                                }, 2000);
                                                event.stopPropagation();
                                            }
                                        });
                                    }

                                    var checkGeo = (localStorage.getItem('geo_location') == null || ".check" == localStorage.getItem('geo_location')) ? 'ton-act' : '';
                                    if (checkGeo != "")
                                    {
                                        document.querySelector('.geo_pos').classList.add(checkGeo);
                                        document.querySelector('.accept_geo').style.display = 'block';
                                        setTimeout(function () {
                                            $('.accept_geo').css('display','none');
                                        }, 4000);
                                    }
                                    geoCheck();
                                });

                                var geo_btns = document.querySelectorAll('.geo_btns > a');
                                for (var i = 0; i < geo_btns.length; i++) {
                                    geo_btns[i].addEventListener('click', function (e) {
                                        e.preventDefault();
                                        if(this.getAttribute('data-answer') == 'yes')
                                            localStorage.setItem('geo_location', "nl.yes");
                                        if(this.getAttribute('data-answer') == 'no')
                                            localStorage.setItem('geo_location', "nl.no");
                                        geoCheck();
                                        setTimeout(function(){
                                            document.querySelector('.geo_pos').classList.remove('ton-act');
                                        },5000);
                                    });
                                }

                                function geoCheck() {
                                    if("nl.yes" == localStorage.getItem('geo_location'))
                                        document.querySelector('.accept_geo').classList.add('yes');
                                    if("nl.no" == localStorage.getItem('geo_location'))
                                        document.querySelector('.accept_geo').classList.add('no');
                                }

                                if("nl.yes" != localStorage.getItem('geo_location') && "nl.no" != localStorage.getItem('geo_location'))
                                    localStorage.setItem('geo_location', "nl.check");
                            </script>
                        
