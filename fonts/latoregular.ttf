    <script>
        var link = document.querySelector('link#nightmode');
        if(link != null) {
            if(document.querySelector('.theme > a') != null) {
                document.querySelectorAll('.theme > a').forEach(function (item) {
                    item.addEventListener('click', function (e) { // Toggle theme
                        e.preventDefault();

                        if (!link) return;
                        link.disabled = !link.disabled;
                        document.cookie = "night_mode="+ +!link.disabled+';path=/';

                        document.querySelectorAll('.theme').forEach(function (theme) {
                            theme.className = !link.disabled ? 'theme white-skins' : 'theme black-skins';
                        });

                        if ('dataLayer' in window){
                            dataLayer.push({'event':'change_theme_scheme', 'eventAction': 'change', 'eventLabel': !link.disabled ? 'theme white-skins' : 'theme black-skins'});
                        }
                    });
                    ['DOMContentLoaded', 'pageshow'].forEach(function(item){
                        window.addEventListener(item, function(){
                            link.disabled = !parseInt(getCookie('night_mode'));
                        });
                    });
                })
            }

            function getCookie(name) {
                var value = "; " + document.cookie;
                var parts = value.split("; " + name + "=");
                if (parts.length == 2) return parts.pop().split(";").shift();
            }
        }
    </script>
