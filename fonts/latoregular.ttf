    <script>
        document.addEventListener("DOMContentLoaded", function() {
            var $nav = $('.update-nav');
            var $btn = $('.burger-menu');
            var $vlinks = $('.menu');
            var $hlinks = $('.hidden-menu');
            var navWidth;
            var vLinksWidth;


            function updateNav() {
                $nav = $('.update-nav');
                $vlinks = $('.menu');
                navWidth = $nav.width() - 200;
                vLinksWidth = $vlinks.width();
                if(vLinksWidth > navWidth) {
                    if($vlinks.children().length > 1) {
                        $vlinks.children().last().prependTo($hlinks);
                        updateNav();
                        }
                } else {
                    if (navWidth > vLinksWidth) {
                        if($hlinks.children().length <= 0) {return;}
                        $hlinks.children().first().appendTo($vlinks);
                        if(vLinksWidth < navWidth-150) {
                            updateNav();
                        }
                    }
                }

                if($hlinks.children().length <= 0) {
                    $btn.addClass('d-none');
                } else {
                        $btn.removeClass('d-none');
                }

            }

            $(function(){
                updateNav();
                $nav.addClass('after');
                $('.new-tab .tabs-button').css('opacity','1');

                var timer;
                $btn.on("mouseover", function() {
                    clearTimeout(timer);
                    openSubmenu();
                }).on("mouseleave", function() {
                    timer = setTimeout(
                        closeSubmenu
                        , 300);
                });
                function openSubmenu() {
                    $hlinks.addClass('show-active');
                }
                function closeSubmenu() {
                    $hlinks.removeClass('show-active');
                }

            });

            $(window).resize(function() {
                    updateNav();
            });
        });
    </script>
